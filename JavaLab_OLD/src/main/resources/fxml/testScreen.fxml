            <TextField fx:id="testTabs3Textfield" maxWidth="40.0" minWidth="30.0" prefWidth="40.0" text="15" />
            <Label text="Кол-во тестов:">
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <TextField fx:id="testTestTextfield" layoutX="470.0" layoutY="25.0" maxWidth="40.0" minWidth="30.0" prefWidth="40.0" text="3" />
