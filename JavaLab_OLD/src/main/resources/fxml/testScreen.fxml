   <items>
      <HBox alignment="CENTER" depthTest="DISABLE" maxHeight="60.0" minHeight="60.0" prefHeight="60.0" spacing="10.0">
         <children>
            <Label text="Кол-во окон:" />
            <TextField fx:id="testWindowsTextfield" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="3" />
            <Label text="Кол-во вкладок в окне:">
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <TextField fx:id="testTabs1Textfield" maxWidth="40.0" minWidth="30.0" prefWidth="40.0" text="5" />
