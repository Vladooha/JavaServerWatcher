<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karmazin.controller.LoginScreen">
	<children>
		<HBox alignment="CENTER_RIGHT" layoutX="130.0" layoutY="149.0" prefHeight="51.0" prefWidth="311.0" style="-fx-spacing: 15; -fx-padding: 10;">
			<children>
				<Label text="Логин:" />
				<TextField fx:id="loginLoginTextfield" />
			</children>
		</HBox>
		<HBox alignment="CENTER_RIGHT" layoutX="130.0" layoutY="200.0" prefHeight="51.0" prefWidth="311.0" style="-fx-spacing: 15; -fx-padding: 10;">
			<children>
				<Label text="Пароль:" />
				<PasswordField id="loginPasswordField" fx:id="loginPasswordPassfield" />
			</children>
		</HBox>
		<Label layoutX="215.0" layoutY="116.0" text="Авторизация">
			<font>
				<Font name="Lobster" size="25.0" />
			</font>
		</Label>
		<ImageView fitHeight="100.0" fitWidth="100.0" layoutX="20.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="/pngs/budgie3.png" />
			</image>
		</ImageView>
		<HBox layoutX="170.0" layoutY="260.0" spacing="30.0">
			<children>
				<Button id="loginSignInButton" fx:id="loginSignInButton" defaultButton="true" mnemonicParsing="false" text="Войти" />
				<Button id="loginRegistrationButton" fx:id="loginRegButton" mnemonicParsing="false" text="Зарегистрироваться" />
			</children>
			<padding>
				<Insets left="10.0" right="10.0" />
			</padding>
		</HBox>
	</children>
</AnchorPane>
