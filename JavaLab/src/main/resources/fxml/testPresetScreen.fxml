<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karmazin.controller.TestPreset">
   <children>
      <VBox alignment="CENTER" layoutX="-1.0" prefHeight="400.0" prefWidth="606.0" spacing="15.0">
         <children>
            <Label text="Test settings" textAlignment="CENTER">
               <font>
                  <Font name="Comic Sans MS" size="27.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" depthTest="DISABLE" prefHeight="57.0" prefWidth="606.0" spacing="10.0">
               <children>
                  <VBox alignment="CENTER_RIGHT" prefHeight="62.0" prefWidth="183.0" spacing="20.0">
