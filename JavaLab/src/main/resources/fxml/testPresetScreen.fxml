                     </children>
                  </VBox>
                  <VBox prefHeight="62.0" prefWidth="173.0" spacing="10.0">
                     <children>
                        <TextField fx:id="testWindowsTextfield" />
                        <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                           <children>
                              <TextField fx:id="testTabs1Textfield" />
                              <TextField fx:id="testTabs2Textfield" />
                              <TextField fx:id="testTabs3Textfield" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button fx:id="testOkButton" defaultButton="true" mnemonicParsing="false" text="Start" />
         </children>
      </VBox>
   </children>
